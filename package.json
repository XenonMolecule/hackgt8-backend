{
  "name": "hackgt8-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\"",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --fix",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "dev": "npm run watch-debug",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "run:ts-node:script": "cross-env ts-node -O {\\\"module\\\":\\\"commonjs\\\"} -r module-alias/register",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint \"./**/*.{tsx,ts,js,jsx}\" --fix --cache"
  },
  "dependencies": {
    "@google-cloud/vision": "^2.4.0",
    "@types/express": "^4.17.13",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "errorhandler": "^1.5.1",
    "express": "~4.16.1",
    "express-fileupload": "^1.2.1",
    "express-jwt": "^6.1.0",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jwks-rsa": "^2.0.5",
    "jwt-decode": "^3.1.2",
    "lusca": "^1.7.0",
    "mongodb-memory-server": "^7.5.1",
    "mongoose": "^6.0.12",
    "morgan": "~1.9.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "^1.5.0",
    "@types/express-session": "^1.17.4",
    "@types/lusca": "^1.7.1",
    "@types/uuid": "^8.3.1"
  }
}
